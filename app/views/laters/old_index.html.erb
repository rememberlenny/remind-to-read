<div class='large-4 column medium-3'>
  <%= render 'layouts/side_nav' %>
</div>
<div class='large-8 column medium-9'>
  <div class='row'>
    <h2>
      Old Feed
    </h2>
    <hr>
  </div>
  <% @laters.each do |later| %>
    <div class='row old-post'>
      <h5>
        <% if !later.title.empty? %>
          <%= later.title %>
        <% else %>
          <%= later.url %>
        <% end %>
        <small>
          Ran at
          <%= later.destined_at.strftime("%D %I:%M") %>
        </small>
      </h5>
    </div>
  <% end %>
</div>